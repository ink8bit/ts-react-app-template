{
  "name": "ts-react-app",
  "version": "0.23.0",
  "description": "TypeScript React app template",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "clean": "rm -rf build",
    "prebuild": "npm run clean",
    "prestart": "npm run build",
    "start": "NODE_ENV=production cd ./build && npx serve",
    "dev": "rollup -c -w",
    "build": "NODE_ENV=production rollup -c",
    "type:check": "tsc --noEmit",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx .",
    "lint:css": "stylelint \"src/**/*.css\"",
    "prettier": "prettier \"./src/**/*.{js,jsx,ts,tsx,css,html}\"",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --list-different",
    "test": "jest --forceExit",
    "test:update": "jest -u",
    "test:watch": "npm t -- --watch",
    "test:ci": "npm t -- --config jest.config.ci.js",
    "prepare": "node ./prepare-hooks.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ink8bit/ts-react-app.git"
  },
  "author": "ink8bit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ink8bit/ts-react-app/issues"
  },
  "homepage": "https://github.com/ink8bit/ts-react-app#readme",
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.7",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-html": "^1.0.2",
    "@rollup/plugin-image": "^3.0.2",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-strip": "^3.0.4",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/babel__traverse": "^7.20.6",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.20.0",
    "cssnano": "^6.1.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "react-test-renderer": "^18.3.1",
    "rollup": "^4.19.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^3.0.0",
    "serve": "^14.2.3",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended": "^13.0.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1"
  }
}
